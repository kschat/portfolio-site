package  {		public class Question {		private var question:String;		private var answer:String;				public function Question(q:String = "", a:String = "") {			this.question = q;			this.answer = a;		}				public function setQuestion(q:String):void {			this.question = q;		}				public function getQuestion():String {			return question;		}				public function setAnswer(a:String):void {			this.answer = a;		}				public function getAnswer():String {			return answer;		}				public function toString():String {			return this.question + " " + this.answer;		}	}}